{
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": ["ES2022", "dom", "dom.iterable"],
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "types": ["react", "node"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "strict": true
  },
  "include": ["."],
  "exclude": ["node_modules"]
}
